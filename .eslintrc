{
    "env": {
      "browser": true,
      "es2021": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
      "ecmaVersion": 12
    },
    "rules": {
      "no-unused-vars": "warn",
      "no-console": "off",
      "no-var": "error",
      "prefer-const": "error",
      "dot-notation": "warn",
      "quotes": ["error", "single"],
      "semi": ["error", "always"],
      "object-shorthand": ["warn", "always"],
      "no-dupe-keys": "error"
    }
  }